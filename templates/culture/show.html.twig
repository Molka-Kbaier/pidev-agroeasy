{% extends 'base.html.twig' %}

{% block title %}Culture{% endblock %}

{% block body %}
    <h1>Culture</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ culture.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ culture.type }}</td>
            </tr>
            <tr>
                <th>Date_planting</th>
                <td>{{ culture.datePlanting ? culture.datePlanting|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Quantite</th>
                <td>{{ culture.quantite }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_culture_index') }}">back to list</a>

    <a href="{{ path('app_culture_edit', {'id': culture.id}) }}">edit</a>

    {{ include('culture/_delete_form.html.twig') }}
{% endblock %}
